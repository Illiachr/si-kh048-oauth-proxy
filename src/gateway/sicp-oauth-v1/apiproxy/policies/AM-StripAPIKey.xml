<AssignMessage name="AM-StripAPIKey">
    <DisplayName>Remove API Key</DisplayName>
    <Remove>
        <Headers>
            <Header name="apikey"/>
        </Headers>
    </Remove>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"></AssignTo>
</AssignMessage>
