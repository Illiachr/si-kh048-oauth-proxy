<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<KeyValueMapOperations name="KVM-PutClientAuth" mapIdentifier="ClientAuth">
  <DisplayName>KVM-PutClientAuth</DisplayName>
  <ExpiryTimeInSecs>43200</ExpiryTimeInSecs>
  <Scope>environment</Scope>
  <Put override="true">
      <Key>
        <Parameter ref="userGrant.login"/>
      </Key>
      <Value ref="private.jwt.token"/>
  </Put>
</KeyValueMapOperations>

